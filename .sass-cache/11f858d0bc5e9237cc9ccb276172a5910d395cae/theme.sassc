o:Sass::Tree::RootNode	:@template"O!@import "compass/reset"
@import "compass/css3"
@import "global"
@import "defaults"
@import "header"
@import "pages"
@import "aside"
@import "entries"
@import "forms"
@import "comments"


// SOME PSEUDO VARIABLES ----------------------------------------------
.orange
  color: #f26649
.p_color
  color: #696a6c
.black
  color: #122632

// @EXTEND
.border-box
  +box-sizing(border-box)
.overflow-hidden
  overflow: hidden
.orange_button
  @extend .border-box
  +text-shadow(#b32609 0 1px 1px)
  +box-shadow(#b32609 0 1px 1px)
  +background(linear-gradient($orange, darken($orange, 5%)))
  +border-radius(4px)
  +inline-block
  text-decoration: none
  height: 40px
  line-height: 40px
  padding: 0 30px
  color: #fff
  font-weight: 600
  border: none

body
  font-size: 14px
  line-height: 1.5em
  font-family: "Helvetica Neue", Helvetica, Arial, "Lucida Grande", sans-serif
  font-weight: 400
  background: #8f9094 url(../images/bletchley_bg.jpg) no-repeat center top fixed
  padding: 0px
  margin: 0px
  padding-bottom: 210px
  position: relative

#main
  border-top: 4px solid $orange
  background: rgba(255,255,255,.95)
  overflow: hidden
  margin: 0 auto
  max-width: 960px
  clear: both
  position: relative
  z-index: 5
  //padding: 1.625em 0 0

#primary
  //float: left
  //margin: 0 -26.4% 0 0
  width: 100%

#content
  //margin: 0 34% 0 7.6%
  //width: 58.4%
  @extend .border-box
  float: left
  padding: 30px 20px 20px 10px
  width: 630px
  border-right: 1px solid lighten($grey, 45%)
  //background: transparentize($magenta, .6)

#secondary
  @extend .border-box
  float: right
  //margin-right: 7.6%
  padding: 20px
  width: 18.8%
  min-width: 330px
  
// NAVIGATION
#content 
  nav
    clear: both
    overflow: hidden
    padding: 0
    margin: 0
    a
      font-size: 12px
      font-weight: bold
      line-height: 2.2em
    &.site-navigation 
    &.paging-navigation
    &#nav-above
      display: none
    &#nav-below
      padding: 0 0 0 120px
    .nav-previous a, .nav-next a
      font-size: 16px
      color: #696A6C
      span.meta-nav
        
.paged #nav-above
  display: block

.nav-previous
  float: left
  width: 50%

.nav-next
  float: right
  text-align: right
  width: 50%

#content .meta-nav
  font-weight: normal

//  Singular navigation
#nav-single
  float: right
  position: relative
  top: -0.3em
  text-align: right
  z-index: 1
  .nav-previous
    float: none
    width: auto
  .nav-next
    float: none
    width: auto
    padding-left: .5em

// SINGULAR
.singular
  #primary
    margin: 0
  #content
    margin: 0 7.6%
    position: relative
    width: auto

.left-sidebar.singular #content
  margin: 0 7.6%
  position: relative
  width: auto

// ALIGNMENT
.alignleft
  display: inline
  float: left
  margin-right: 1.625em

.alignright
  display: inline
  float: right
  margin-left: 1.625em

.aligncenter
  clear: both
  display: block
  margin-left: auto
  margin-right: auto

// HEADINGS
h1, h2, h3, h4, h5
  line-height: 1.2em
  margin: 0 0 1em 0
  clear: both
h2
  font-size: 30px
h3
  color: #303236
  font-size: 18px
  font-weight: bold
  margin-bottom: 15px
hr
  background-color: #ccc
  border: 0
  height: 1px
  margin-bottom: 1.625em

// MISC
embed, iframe, object
  max-width: 100%


// LINKS
#content a
  &:focus img, &:hover img, &:active img
    //  Add some useful style to those fancy borders for linked images ...
    background: #eee
    border-color: #bbb
a
  color: $orange
  text-decoration: none
  &:focus, &:active, &:hover
    text-decoration: underline
.page-link
  clear: both
  display: block
  margin: 0 0 1.625em
  a
    background: #eee
    color: #373737
    margin: 0
    padding: 2px 3px
    text-decoration: none
    &:hover
      background: #888
      color: #fff
      font-weight: bold
  span
    margin-right: 6px
a
  &:focus img[class*="align"], &:hover img[class*="align"], &:active img[class*="align"], &:focus img[class*="wp-image-"], &:hover img[class*="wp-image-"], &:active img[class*="wp-image-"]
    //  Add some useful style to those fancy borders for linked images ...
    background: #eee
    border-color: #bbb

// IMAGES
.widget img
  max-width: 97.5%
  //  Fluid images for posts, comments, and widgets
img
  &[class*="align"], &[class*="wp-image-"], &[class*="attachment-"]
    height: auto
    //  Make sure images with WordPress-added height and width attributes are scaled correctly
  &.size-full, &.size-large
    max-width: 97.5%
    width: auto
    //  Prevent stretching of full-size and large-size images with height and width attributes in IE8
    height: auto
    //  Make sure images with WordPress-added height and width attributes are scaled correctly
img
  &.alignleft, &.alignright, &.aligncenter
    margin-bottom: 1.625em
p img
  margin-top: 0.4em
#content img
  border: none
  max-width: 100%
  padding: 0
img#wpstats
  display: block
  margin: 0 auto 1.625em
img
  &[class*="align"], &[class*="wp-image-"]
    border: 1px solid #ddd
    padding: 6px
#content img
  border: 1px solid #ddd
  padding: 6px
#content img
  border: none
  max-width: 100%
  padding: 0

// CAPTIONS
.wp-caption
  margin-top: 0.4em
  background: #eee
  margin-bottom: 1.625em
  max-width: 96%
  padding: 9px
  img
    display: block
    margin: 0 auto
    max-width: 98%
  .wp-caption-text
    color: #666
    font-family: Georgia, serif
    font-size: 12px
.wp-caption .wp-caption-text
  margin-bottom: 0.6em
  padding: 10px 0 5px 40px
  position: relative
  &:before
    color: #666
    content: '\2014'
    font-size: 14px
    font-style: normal
    font-weight: bold
    margin-right: 5px
    position: absolute
    left: 10px
    top: 7px
.wp-caption img
  border-color: #eee
.wp-caption a
  &:focus img, &:active img, &:hover img
    //  ... including captioned images!
    background: #fff
    border-color: #ddd

// SINGULAR CONTENT STYLES FOR POSTS AND PAGES
blockquote.pull
  font-size: 21px
  font-weight: bold
  line-height: 1.6125em
  margin: 0 0 1.625em
  text-align: center
.singular blockquote.pull
  margin: 0 -22.25% 1.625em
.pull.alignleft
  margin: 0 1.625em 0 0
  text-align: right
  width: 33%
.singular .pull.alignleft
  margin: 0 1.625em 0 -22.25%
.pull.alignright
  margin: 0 0 0 1.625em
  text-align: left
  width: 33%
.singular
  .pull.alignright
    margin: 0 -22.25% 0 1.625em
  blockquote.pull
    &.alignleft, &.alignright
      width: 33%
  .entry-meta a
    bottom: auto
    left: 50px
    position: absolute
    right: auto
    top: 80px
.format-quote blockquote
  color: #555
  font-size: 17px
  margin: 0
.indexed.format-image
  .wp-caption
    background: #111
    margin-bottom: 0
    max-width: 96%
    padding: 11px
    .wp-caption-text
      color: #ddd
      &:before
        color: #444
  a:hover img
    opacity: 0.8

// INTRO
article.intro
  background: #f9f9f9
  border-bottom: none
  margin: -1.855em -8.9% 1.625em
  padding: 0 8.9%
  .entry-title
    display: none
  .entry-content
    color: #111
    font-size: 16px
    padding: 1.625em 0 0.625em
  .edit-link a
    background: #aaa
    color: #fff
    font-size: 12px
    padding: 0 8px
    position: absolute
    top: 30px
    right: 20px
    text-decoration: none
    &:hover, &:focus, &:active
      background: #777

.form-allowed-tags
  display: none

#colophon
  clear: both

#supplementary
  border-top: 1px solid #ddd
  padding: 1.625em 7.6%
  overflow: hidden
  &.two .widget-area
    float: left
    margin-right: 3.7%
    width: 48.1%
    + .widget-area
      margin-right: 0
  &.three .widget-area
    float: left
    margin-right: 3.7%
    width: 30.85%
    + .widget-area
      margin-right: 0

//  =IE7
#ie7
  article.intro
    margin-left: -7.6%
    margin-right: -7.6%
    padding-left: -7.6%
    padding-right: -7.6%
    max-width: 1000px
  section
    &.featured-post
      margin-left: -7.6%
      margin-right: -7.6%
      max-width: 850px
    &.recent-posts
      margin-right: 7.6%

footer#colophon
  @extend .border-box
  height: 250px
  background: #696A6C
  padding-top: 95px
  position: absolute
  bottom: 0px
  left: 0px
  right: 0px
  z-index: 0
  .site-info
    font-size: 16px
    color: #C8CBD0
    margin: 0 auto
    max-width: 960px
    overflow: hidden
  img.logo
    float: left
    margin-right: 70px
  a
    display: block
    color: #C8CBD0
    text-decoration: underline
    &:after
      content: $arrow_right
      margin-left: 3px
      text-decoration: none
  .column
    width: 240px
    float: left
    p.copyright
      font-size: 12px
      color: #AEB1B7
      margin-top: 50px








